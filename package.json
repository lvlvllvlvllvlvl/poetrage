      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "bracketSameLine": true
  },
  "lint-staged": {
    "{src,public}/**": [
      "organize-imports-cli",
      "prettier --write"
